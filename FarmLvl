if game.PlaceId == 4442272183 then
local End = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")

End.Name = "End"
End.Parent = game:GetService("CoreGui")

Frame.Parent = End
Frame.BackgroundColor3 = Color3.fromRGB(0, 255, 0)

coroutine.wrap(function()
while wait(1) do
if game.Players.LocalPlayer.Backpack:FindFirstChild("Black Leg") then
game.Players.LocalPlayer.Backpack:FindFirstChild("Black Leg").Parent = game.Players.LocalPlayer.Character
end
if game:GetService("Players")["LocalPlayer"].Data.Level.Value >= 700 then
break
end
end
end)()
